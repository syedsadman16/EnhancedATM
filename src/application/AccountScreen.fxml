<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AccountScreenController">
   <children>
      <AnchorPane prefHeight="600.0" prefWidth="600.0">
         <children>
            <Label layoutX="48.0" layoutY="65.0" text="Hello,">
               <font>
                  <Font size="33.0" />
               </font>
            </Label>
            <Label fx:id="nameLabel" layoutX="138.0" layoutY="65.0" text="Label">
               <font>
                  <Font name="System Italic" size="33.0" />
               </font></Label>
            <Button fx:id="wdrawBtn" layoutX="226.0" layoutY="238.0" mnemonicParsing="false" onAction="#goToTransactionScreen" text="Withdraw" />
            <Button fx:id="DepBtn" layoutX="236.0" layoutY="313.0" mnemonicParsing="false" onAction="#goToDepositScreen" text="Deposit" />
            <Button fx:id="asumBtn" layoutX="208.0" layoutY="392.0" mnemonicParsing="false" onAction="#goToSummaryScreen" text="Account Summary" />
            <Button fx:id="signOutBtn" layoutX="235.0" layoutY="453.0" mnemonicParsing="false" onAction="#signOut" text="Settings" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
